{
  "name": "with-redux-toolkit",
  "version": "1.0.0",
  "scripts": {
    "dev": "concurrently next npm:styleTypings",
    "build": "next build",
    "start": "next start",
    "test": "jest",
    "styleTypings": "tsm src -w --includePaths src/styles --nameFormat dashes",
    "type-check": "ttsc --noEmit",
    "prettier:check": "prettier \"{pages,src}/**/*.{ts,tsx,js,jsx}\"",
    "prettier": "npm run prettier:check -- --write",
    "eslint:check": "eslint \"{pages,src}/**/*.{ts,tsx}\"",
    "eslint": "npm run eslint:check -- --fix"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.4.0",
    "clsx": "^1.1.1",
    "lodash": "^4.17.20",
    "next": "^9.5.5",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.1"
  },
  "license": "MIT",
  "devDependencies": {
    "@testing-library/dom": "^7.26.0",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.0.4",
    "@types/concurrently": "^5.2.1",
    "@types/eslint": "^7.2.4",
    "@types/eslint-plugin-prettier": "^3.1.0",
    "@types/lodash": "^4.14.162",
    "@types/node": "^14.11.8",
    "@types/node-sass": "^4.11.1",
    "@types/prettier": "^2.1.2",
    "@types/react": "^16.9.52",
    "@types/react-dom": "^16.9.8",
    "@types/react-redux": "^7.1.9",
    "@types/testing-library__jest-dom": "^5.9.4",
    "@typescript-eslint/eslint-plugin": "^4.4.1",
    "@typescript-eslint/parser": "^4.4.1",
    "babel-jest": "^26.5.2",
    "concurrently": "^5.3.0",
    "eslint": "^7.11.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.4",
    "jest": "^26.5.3",
    "jest-dom": "^4.0.0",
    "node-sass": "^4.14.1",
    "prettier": "^2.1.2",
    "typed-scss-modules": "^3.2.1",
    "typescript": "^4.0.3"
  },
  "browserslist": [
    ">0.3%",
    "not ie 11",
    "not dead",
    "not op_mini all"
  ]
}